service face_hal /vendor/bin/hw/vendor.oppo.hardware.biometrics.face@1.0-service
    # "class hal" causes a race condition on some devices due to files created
    # in /data. As a workaround, postpone startup until later in boot once
    # /data is mounted.
    class late_start
    user system
    group system input sdcard_rw sdcard_
    onrestart restart oppo_face_sh
